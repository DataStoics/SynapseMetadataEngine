{
	"name": "SQLServer",
	"properties": {
		"parameters": {
			"SqlServerName": {
				"type": "String",
				"defaultValue": "rpadfmetadatademo.database.windows.net"
			},
			"SqlDatabaseName": {
				"type": "string",
				"defaultValue": "AdventureWorks "
			},
			"sqlusername": {
				"type": "string",
				"defaultValue": "repajta"
			},
			"kvsecretname": {
				"type": "string",
				"defaultValue": "SQLServer-AdventureWorks-Secret"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Data Source=@{linkedService().SqlServerName};Initial Catalog=@{linkedService().SqlDatabaseName};User ID=@{linkedService().sqlusername}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault1",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().kvsecretname",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.Synapse/workspaces/linkedservices"
}